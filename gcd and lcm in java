import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
	    int gcd=1,lcm=0;
		Scanner sc=new Scanner(System.in);
		int a=sc.nextInt();
		int b=sc.nextInt();
		for(int i=1;i<=a && i<=b;++i)
		{
		    if(a%i==0 && b%i==0)
		    {
		        gcd=i;
		    }
		}
	    lcm=(a * b) / gcd;
		System.out.println("the value of gcd is :"+gcd);
		System.out.println("the value of lcm is :"+lcm);
	}
}
